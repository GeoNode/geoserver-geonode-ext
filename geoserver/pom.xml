    <repositories>

      <repository>
        <id>JBoss repo</id>
        <url>http://repository.jboss.org/nexus/content/groups/public-jboss/</url>
      </repository>

      <repository>
        <id>org.mapfish</id>
        <url>http://dev.mapfish.org/maven/repository</url>
