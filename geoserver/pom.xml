        <hibernate-annotations-version>3.3.1.GA</hibernate-annotations-version>
		<hibernate-generic-dao-version>1.1.0</hibernate-generic-dao-version>
		<hibernate-spatial-version>1.1.1</hibernate-spatial-version>
